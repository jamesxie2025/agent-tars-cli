# Agent TARS CLI Configuration - OpenAI GPT-4o
# Copy this file to .env to use OpenAI model

# OpenAI API Key
OPENAI_API_KEY=your-openai-api-key

# ModelScope API Key (optional, for switching)
MODELSCOPE_API_KEY=your-modelscope-api-key

# DeepSeek API Key (optional, for switching)
DEEPSEEK_API_KEY=your-deepseek-api-key

# Model Configuration - GPT-4o
TARS_MODEL_PROVIDER=openai
TARS_MODEL_NAME=gpt-4o
TARS_MODEL_BASE_URL=
TARS_MODEL_API_KEY=${OPENAI_API_KEY}

# Server Configuration
PORT=8888
CACHE_DIR=./cache
DATA_DIR=./data
GENERATED_DIR=./generated

